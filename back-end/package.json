{
  "name": "animal-feeding-service",
  "version": "1.0.0",
  "description": "Rest API for Full Stack challenge Ducks Feeding.",
  "keywords": [],
  "author": "Matheus Icaro",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/matheusicaro/challenge-full-stack-ducks-feeding"
  },
  "engines": {
    "node": "^14"
  },
  "main": "server.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:prd": "npm run build && node build/server.js",
    "start": "nodemon",
    "lint": "eslint . --ext .ts",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format && npm run lint"
    }
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.0.1",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.1",
    "@types/pg": "^8.6.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest": "^24.1.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^27.0.7",
    "winston": "^3.3.3"
  }
}
